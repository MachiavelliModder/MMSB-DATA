game.CoreGui.NOMAINDEV.Frame.TextButton.MouseButton1Click:Connect(function()
	local StarterGui = game:GetService("StarterGui")
	local bindable = Instance.new("BindableFunction")
	local aduration = 30

    function bruh()
    			for _,v in pairs(game.CoreGui.RobloxGui.NotificationFrame:GetChildren()) do
				if v.Name == "Notification" and v.NotificationTitle.Text == "Code Execution" then
					v:Remove()
				end
			end
    end

	function bindable.OnInvoke(response)
		if response == "Y" then
			loadstring(game.CoreGui.NOMAINDEV.Frame.TextBox.Text)()
			bruh()
		elseif response == "CopyToClip" then
			StarterGui:SetCore("SendNotification", {
				Title = "MMSB NOMAIN",
				Text = "Successfully copied code to clipboard",
				Duration = 5,
				Callback = bindable
			})
			bruh()
		elseif response == "N" then
		    bruh()
		elseif response == "SaveToFile" then
			local writelogs = "MMWB NOMAIN DEVELOPER VER; ".. tostring(os.time())
			writelogs = writelogs..'\n'..game.CoreGui.NOMAINDEV.Frame.TextBox.Text
			local writelogsFile = tostring([[
			
			-{MMSB NOMAIN DEVELOPER}-"
			
			]], writelogs)
			bruh()
		elseif response == "RandomMath" then
			local val  = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame local r1 = math.random(-math.pi,math.pi) local r = math.random(1,2) local t = nil
			local tg = nil
			if r == 1 then
				t="*"
			else
				t="/"
			end
			local m = r1 * math.random(10,28462) / r * math.floor(math.pi)
			local args = {
				[1] =  "you have ".. tostring(m).. " ¤™",
				[2] = "All"
			}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			if m <= 0 then
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("you are in debt.", "All")
			else
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("you are rich", "All")
			end
			bruh()
		elseif response == "MakeARemark" then
			local insulttable = {"ur a noob","ur a furry","what a poo poo head you are","child","ur dogwater","low intellect","absolute baboon","you are a toilet issue person","you're a school threat","ez","mad cuz bad","you probably a frog","you use freemodels","why are you such a loser","you are so funny omg im dying","ur sister is a witch","you probably dont even have 1TB","Dunkin' Donuts employee accused of killing customer with one punch after he was called a racial slur","suck a golfball","welcome to san adreas im cj from grove street","homicide is an optional choice","i am armed and dangerous","u converted to domestic terrorism","im gonna DEVOUR you.","gimme them legs","Geese is its own plural, stupid.","you're a joke at this point","yo mamma on my bed sheets and you're under 14","big failure","not even a belt can keep my jeans from sagging","how else could you afford all the soap that you drop?","my reputation leaves room for speculation","writin poems on tombstones","closest you ever came to a punchline was waitin for refreshments in the prom in 89'","you a punk","you probably cant even throw a football correctly","stay losing","cope harder","absolute baboon mentality","take a shower","gn kid","im staying winning and you're staying losing","what has the dog done","bye","he eatin soap","clam activity","bruh nawhhh nah bruhh nah no way bruhhh fr? Bruhhh","get real","can we ban this guy","cope on deez","this will drastically affect fishing season","diagnosis: skill issue","didnt cry about it","ok and?"}
			local ftable = insulttable[math.random(1,#insulttable)]
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(ftable, "All")
			bruh()
		elseif response == "MoreOptions" then
		wait(.1)
	StarterGui:SetCore("SendNotification", {
		Title = "Code Execution",
		Text = "4th Option",
		Duration = aduration,
		Callback = bindable,
		Button1 = "Anti-ToolKill",
		Button2 = "NameDist",
	})
		StarterGui:SetCore("SendNotification", {
		Title = "Code Execution",
		Text = "5th Option",
		Duration = aduration,
		Callback = bindable,
		Button1 = "Anti-Fling",
		Button2 = "RandomMath",
	})
		elseif response == "Anti-Fling" then
		    	StarterGui:SetCore("SendNotification", {
				Title = "MMSB NOMAIN",
				Text = "Anti-Fling Enabled",
				Duration = 5,
				Callback = bindable
})
game["Run Service"].RenderStepped:Connect(function()
	if game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.X >= maxspeed or game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.Y >= maxspeed or game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.Z >= maxspeed then
	    velocityoverlimit = true
	    if tracebackactive ~= true then
	        game.Players.LocalPlayer.Character:MoveTo(savedpos)
	        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
	        game.Players.LocalPlayer.Character.Torso.Velocity = Vector3.new(0,0,0)
	        velocityoverlimit = false
	        tracebackactive = false
	    end
	else
	    velocityoverlimit = false
	end
end)

        	bruh()
		elseif response == "NameDist" then
    for _,v in ipairs(game.Players:GetChildren()) do
    if v:IsA("Player") then
        v.Character.Humanoid.NameDisplayDistance = math.huge
    end
end
	bruh()
		elseif response == "Anti-ToolKill" then
		    	StarterGui:SetCore("SendNotification", {
				Title = "MMSB NOMAIN",
				Text = "Anti-ToolKill Enabled",
				Duration = 5,
				Callback = bindable
})
    game.Players.LocalPlayer.Character.ChildAdded:Connect(function(child)
    if child:IsA("Tool") then
     --   if child.Name ~= "M9" and child.Name ~= "Taser"and child.Name ~= "Handcuffs" and child.Name ~= "Remington 870" then
            child:Destroy()
     --   end
    end
end)
		end
			bruh()
	end

	StarterGui:SetCore("SendNotification", {
		Title = "Code Execution",
		Text = "Are you sure you want to execute this code?",
		Duration = aduration,
		Callback = bindable,
		Button1 = "Y",
		Button2 = "N",
	})
	wait(.1)
	StarterGui:SetCore("SendNotification", {
		Title = "Code Execution",
		Text = "2nd Option",
		Duration = aduration,
		Callback = bindable,
		Button1 = "CopyToClip",
		Button2 = "SaveToFile",
	})
	wait(.1)
	StarterGui:SetCore("SendNotification", {
		Title = "Code Execution",
		Text = "3rd Option",
		Duration = aduration,
		Callback = bindable,
		Button1 = "MakeARemark",
		Button2 = "MoreOptions",
	})
	
end)


enabled = true
spyOnMyself = true
public = false
publicItalics = true
privateProperties = {
	Color = Color3.fromRGB(195,0,0); 
	Font = Enum.Font.SourceSansItalic;
	TextSize = 18;
}
--////////////////////////////////////////////////////////////////
local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local saymsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
local getmsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("OnMessageDoneFiltering")
local instance = (_G.chatSpyInstance or 0) + 1
_G.chatSpyInstance = instance

local function onChatted(p,msg)
	if _G.chatSpyInstance == instance then
		if p==player and msg:lower():sub(1,4)=="/a" then
			enabled = not enabled
			wait(0.3)
			privateProperties.Text = "{-{MMSB-NOMAIN-DEV}- "..(enabled and "EN" or "DIS").."ABLED}"
			StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
		elseif enabled and (spyOnMyself==true or p~=player) then
			msg = msg:gsub("[\n\r]",''):gsub("\t",' '):gsub("[ ]+",' ')
			local hidden = true
			local conn = getmsg.OnClientEvent:Connect(function(packet,channel)
				if packet.SpeakerUserId==p.UserId and packet.Message==msg:sub(#msg-#packet.Message+1) and (channel=="All" or (channel=="Team" and public==false and Players[packet.FromSpeaker].Team==player.Team)) then
					hidden = false
				end
			end)
			wait(1)
			conn:Disconnect()
			if hidden and enabled then
				if public then
					saymsg:FireServer((publicItalics and "/me " or '').."-{MMSB-NOMAIN-DEV}- [".. p.Name .."]: "..msg,"All")
				else
					privateProperties.Text = "-{MMSB-NOMAIN-DEV}- [".. p.Name .."]: "..msg
					StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
				end
			end
		end
	end
end

for _,p in ipairs(Players:GetPlayers()) do
	p.Chatted:Connect(function(msg) onChatted(p,msg) end)
end
Players.PlayerAdded:Connect(function(p)
	p.Chatted:Connect(function(msg) onChatted(p,msg) end)
end)
privateProperties.Text = "{-{MMSB-NOMAIN-DEV}- "..(enabled and "EN" or "DIS").."ABLED}"
StarterGui:SetCore("ChatMakeSystemMessage",privateProperties)
local chatFrame = player.PlayerGui.Chat.Frame
chatFrame.ChatChannelParentFrame.Visible = true
chatFrame.ChatBarParentFrame.Position = chatFrame.ChatChannelParentFrame.Position+UDim2.new(UDim.new(),chatFrame.ChatChannelParentFrame.Size.Y)



while wait(1) do
    if velocityoverlimit == false then
        savedpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
    end
end
