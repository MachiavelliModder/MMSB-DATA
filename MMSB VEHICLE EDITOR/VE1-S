-- Scripts:

local function KFVQ_fake_script() -- Frame.Dragify 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 0
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(KFVQ_fake_script)()
local function TZNH_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	SeatInstance = nil
	WheelParent = nil
	Buttons = script.Parent.Frame.Buttons
	
	script.Parent.Frame.FIND.MouseButton1Click:Connect(function()
		for _,o in ipairs(Buttons.Wheels:GetChildren()) do
			if o:IsA("TextButton") then
				o:Destroy()
			end
			wait()
		end
		for _,o in ipairs(script.Parent.Instances:GetChildren()) do
			if o:IsA("ObjectValue") then
				o:Destroy()
			end
			wait()
		end
		for _,a in ipairs(workspace:GetDescendants()) do
			if a:IsA("Seat") or a:IsA("VehicleSeat") then
				if a.Occupant == game.Players.LocalPlayer.Character.Humanoid then
					SeatInstance = a
					script.Parent.Frame.FV.Visible = false
					script.Parent.Frame.FV2.Visible = false
					script.Parent.Frame.Buttons.Visible = true
					for _,b in ipairs(a.Parent:GetDescendants()) do
						if string.find(b.Name,"Wheel") or string.find(b.Name,"wheel") then
							if string.find(b.Name,"Steering") or string.find(b.Name,"Fixed") or string.find(b.Name,"Weld") then
							else
								local gui = script.Parent.Wheel:Clone()
								gui.Parent = script.Parent.Frame.Buttons.Wheels
								gui.Text = b.Name
								gui.Visible = true
								WheelParent = b.Parent
								local ObjectValue = Instance.new("ObjectValue",script.Parent.Instances)
								ObjectValue.Name = b.Name
								ObjectValue.Value = b
								gui.MouseButton1Click:Connect(function()
									script.Parent.Frame.Buttons.SW.Text = gui.Text
									for _,a in ipairs(script.Parent.Instances:GetChildren()) do
										if a.Name == gui.Text then
											for _,m in ipairs(a.Value.Parent.Parent:GetDescendants()) do
												if m:IsA("SpringConstraint") then
													Buttons.LowerButtons.MaxForce.Text = tostring(math.floor(m.MaxForce))
													Buttons.LowerButtons.MinLength.Text = tostring(math.floor(m.MinLength))
													Buttons.LowerButtons.MaxLength.Text = tostring(math.floor(m.MaxLength))
													Buttons.LowerButtons.Stiffness.Text = tostring(math.floor(m.Stiffness))
													Buttons.LowerButtons.Damping.Text = tostring(math.floor(m.Damping))
													Buttons.LowerButtons.FreeLength.Text = tostring(math.floor(m.FreeLength))
												end
											end
										end
									end
								end)
							end
						end
					end
					if WheelParent == nil then
						for _,b in ipairs(a.Parent.Parent:GetDescendants()) do
							if a.Parent.Parent == workspace then
							else
								if string.find(b.Name,"Wheel") or string.find(b.Name,"wheel") then
									if string.find(b.Name,"Steering") or string.find(b.Name,"Fixed") or string.find(b.Name,"Weld") then
									else
										local gui = script.Parent.Wheel:Clone()
										gui.Parent = script.Parent.Frame.Buttons.Wheels
										gui.Text = b.Name
										gui.Visible = true
										WheelParent = b.Parent
										local ObjectValue = Instance.new("ObjectValue",script.Parent.Instances)
										ObjectValue.Name = b.Name
										ObjectValue.Value = b
										gui.MouseButton1Click:Connect(function()
											script.Parent.Frame.Buttons.SW.Text = gui.Text
											for _,a in ipairs(script.Parent.Instances:GetChildren()) do
												if a.Name == gui.Text then
													for _,m in ipairs(a.Value.Parent.Parent:GetDescendants()) do
														if m:IsA("SpringConstraint") then
															Buttons.LowerButtons.MaxForce.Text = tostring(math.floor(m.MaxForce))
															Buttons.LowerButtons.MinLength.Text = tostring(math.floor(m.MinLength))
															Buttons.LowerButtons.MaxLength.Text = tostring(math.floor(m.MaxLength))
															Buttons.LowerButtons.Stiffness.Text = tostring(math.floor(m.Stiffness))
															Buttons.LowerButtons.Damping.Text = tostring(math.floor(m.Damping))
															Buttons.LowerButtons.FreeLength.Text = tostring(math.floor(m.FreeLength))
														end
													end
												end
											end
										end)
									end
								end
							end
						end
					end
					if WheelParent == nil then
