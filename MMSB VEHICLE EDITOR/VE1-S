													for _,m in ipairs(a.Value.Parent.Parent:GetDescendants()) do
														if m:IsA("SpringConstraint") then
															Buttons.LowerButtons.MaxForce.Text = tostring(math.floor(m.MaxForce))
															Buttons.LowerButtons.MinLength.Text = tostring(math.floor(m.MinLength))
															Buttons.LowerButtons.MaxLength.Text = tostring(math.floor(m.MaxLength))
															Buttons.LowerButtons.Stiffness.Text = tostring(math.floor(m.Stiffness))
															Buttons.LowerButtons.Damping.Text = tostring(math.floor(m.Damping))
															Buttons.LowerButtons.FreeLength.Text = tostring(math.floor(m.FreeLength))
														end
													end
												end
											end
										end)
									end
								end
							end
						end
					end
				end
			end
		end
	end)
	
	script.Parent.Frame.Buttons.AW.MouseButton1Click:Connect(function()
		script.Parent.Frame.Buttons.SW.Text = "All Wheels"
	end)
	
	
	script.Parent.Frame.Buttons.Apply.MouseButton1Click:Connect(function()
		if script.Parent.Frame.Buttons.SW.Text == "All Wheels" then
			for _,a in ipairs(script.Parent.Instances:GetChildren()) do
				if a:IsA("ObjectValue") then
					for _,n in ipairs(a.Value.Parent.Parent:GetDescendants()) do
						if n:IsA("SpringConstraint") then
							if n.LimitsEnabled == true then
								n.MaxLength = tonumber(Buttons.LowerButtons.MaxLength.Text)
								n.MinLength = tonumber(Buttons.LowerButtons.MinLength.Text)
								n.MaxForce = tonumber(Buttons.LowerButtons.MaxForce.Text)
								n.Stiffness = tonumber(Buttons.LowerButtons.Stiffness.Text)
								n.Damping = tonumber(Buttons.LowerButtons.Damping.Text)
								n.FreeLength = tonumber(Buttons.LowerButtons.FreeLength.Text)
							end
						end
					end
				end
			end
		else
			local a = script.Parent.Instances:FindFirstChild(script.Parent.Frame.Buttons.SW.Text)
			for _,n in ipairs(a.Value.Parent:GetDescendants()) do
				if n:IsA("SpringConstraint") then
					if n.LimitsEnabled == true then
						n.MaxLength = tonumber(Buttons.LowerButtons.MaxLength.Text)
						n.MinLength = tonumber(Buttons.LowerButtons.MinLength.Text)
						n.MaxForce = tonumber(Buttons.LowerButtons.MaxForce.Text)
						n.Stiffness = tonumber(Buttons.LowerButtons.Stiffness.Text)
						n.Damping = tonumber(Buttons.LowerButtons.Damping.Text)
						n.FreeLength = tonumber(Buttons.LowerButtons.FreeLength.Text)
					end
				end
			end
		end
	end)
end
coroutine.wrap(TZNH_fake_script)()
