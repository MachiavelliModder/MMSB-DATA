local function PHJT_fake_script() -- ARG.LocalScript 
	local script = Instance.new('LocalScript', ARG)

	script.Parent.MouseButton1Click:Connect(function()
	script.Parent.Parent.Parent.Parent.Switch:Play()
	script.Parent.Parent.Parent.Selected.SelectM.PP.Value = false
	script.Parent.Parent.Parent.Selected.SelectM.AU.Value = false
	script.Parent.Parent.Parent.Selected.SelectM.ARG.Value = true
	script.Parent.Parent.Parent.Selected.SelectM.LEG.Value = false
	script.Parent.Parent.Parent.Selected.SelectM.WLD.Value = false
	script.Parent.Parent.Parent.Selected.SelectM.NVM.Value = false
	end)
end
coroutine.wrap(PHJT_fake_script)()
