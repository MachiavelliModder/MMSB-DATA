-- Scripts:

local function XJTJR_fake_script() -- Mainframe.Dragify 
	local script = Instance.new('LocalScript', Mainframe)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 0.50
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(XJTJR_fake_script)()
local function GVJY_fake_script() -- OptionsClose.LocalScript 
	local script = Instance.new('LocalScript', OptionsClose)

	script.Parent.MouseButton1Click:Connect(function()
	script.Parent.Parent.Parent.Parent.ClickFast:Play()
	script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(GVJY_fake_script)()
local function FFSOO_fake_script() -- Changelog.LocalScript 
	local script = Instance.new('LocalScript', Changelog)

	deb = false
	script.Parent.MouseButton1Click:Connect(function()
	if deb == false then
	script.Parent.Frame.Visible = true
	script.Parent.Parent.Parent.OptionsOpen.Visible = false
	deb = true
	elseif deb == true then
	script.Parent.Frame.Visible = false
	script.Parent.Parent.Parent.OptionsOpen.Visible = true
	deb = false
	end
	if deb == true then
	script.Parent.Parent.Parent.Parent.Button:Play()
	else
	script.Parent.Parent.Parent.Parent.Button:Play()
	end
	end)
	
end
coroutine.wrap(FFSOO_fake_script)()
local function ALUOI_fake_script() -- SelectM.LocalScript 
	local script = Instance.new('LocalScript', SelectM)

	confirmed = false
	
	script.Parent.MouseButton1Click:Connect(function()
	if confirmed == false then
	script.Parent.Text = "Confirm"
	script.Parent.Parent.Parent.Parent.Kerplunk:Play()
	script.Parent.Parent.Parent.Mode.Visible = true
	confirmed = true
	elseif confirmed == true then
	if script.Parent.PP.Value == true then
	script.Parent.Text = "Push / Pull"
	confirmed = false
	script.Parent.Parent.Parent.Mode.Visible = false
	script.Parent.Parent.Parent.Parent.Copy:Play()
	elseif script.Parent.AU.Value == true then
	confirmed = false
	script.Parent.Parent.Parent.Mode.Visible = false
	script.Parent.Parent.Parent.Parent.Copy:Play()
	script.Parent.Text = "Anchor / Unanchor"
	elseif script.Parent.ARG.Value == true then
	confirmed = false
	script.Parent.Parent.Parent.Mode.Visible = false
	script.Parent.Parent.Parent.Parent.Copy:Play()
	script.Parent.Text = "Add / Remove Gyro"
	elseif script.Parent.LEG.Value == true then
	confirmed = false
	script.Parent.Parent.Parent.Mode.Visible = false
	script.Parent.Parent.Parent.Parent.Copy:Play()
	script.Parent.Text = "Legacy (All Modes)"
	elseif script.Parent.NVM.Value == true then
	confirmed = false
	script.Parent.Parent.Parent.Mode.Visible = false
	script.Parent.Parent.Parent.Parent.Kerplunk:Play()
	script.Parent.Text = "Select Mode"
	elseif script.Parent.WLD.Value == true then
	confirmed = false
	script.Parent.Parent.Parent.Mode.Visible = false
	script.Parent.Parent.Parent.Parent.Kerplunk:Play()
	script.Parent.Text = "Make / Destroy Weld"
	end
	end
	end)
	script.Parent.PP.Changed:Connect(function()
	if script.Parent.PP.Value == true then
	script.Parent.Text = "Confirm Push/Pull"
	end
	end)
	script.Parent.AU.Changed:Connect(function()
	if script.Parent.AU.Value == true then
	script.Parent.Text = "Confirm Anchor/Unanchor"
	end
	end)
	script.Parent.ARG.Changed:Connect(function()
	if script.Parent.ARG.Value == true then
	script.Parent.Text = "Confirm Add/Remove Gyro"
	end
	end)
	script.Parent.LEG.Changed:Connect(function()
	if script.Parent.LEG.Value == true then
	script.Parent.Text = "Confirm Legacy"
	end
	end)
	script.Parent.WLD.Changed:Connect(function()
	if script.Parent.WLD.Value == true then
	script.Parent.Text = "Confirm Weld"
	end
	end)
	script.Parent.NVM.Changed:Connect(function()
	if script.Parent.NVM.Value == true then
	confirmed = false
	script.Parent.Text = "Select Mode"
	script.Parent.Parent.Parent.Parent.Kerplunk:Play()
	script.Parent.Parent.Parent.Mode.Visible = false
	end
	end)
end
coroutine.wrap(ALUOI_fake_script)()
