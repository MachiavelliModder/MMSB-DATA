    while wait(10) do
        if CombatModuleStatus == "ONLINE" then
            for _,a in pairs(game.Players:GetChildren()) do
            	local ch = coroutine.create(function()
            	    if a:IsA("Player") then
            	        if a.Character:FindFirstChild("TRACKER#042") then
            	            else
            	                local i = Instance.new("BoolValue",a.Character)
            	                i.Name = "TRACKER#042"
            	                
                	        	a.Character.ChildAdded:Connect(function(c)
                	        	  if CombatModuleStatus == "ONLINE" then
                        			local s = string.lower(c.Name)
                        			if string.find(s,"bullet") then
                        				highlightobj(c)
                        			end
                        			end
                        		end)
