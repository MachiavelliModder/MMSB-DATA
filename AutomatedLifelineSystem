                        			local s = string.lower(c.Name)
                        			if string.find(s,"bullet") then
                        				highlightobj(c)
                        			end
                        			end
                        		end)
                    		end
            	        end
        		    end
            	end)
            	coroutine.resume(ch)
            end
        end
    end
end)
coroutine.resume(m)


