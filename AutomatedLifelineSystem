local player = game:GetService("Players").LocalPlayer
local mouse = player:GetMouse()
local core = game.CoreGui.RobloxGui.Auto


local consoleenabled = true


local EmergencyModuleStatus = "ONLINE"
local CombatModuleStatus = "ONLINE"
local AgilityModuleStatus = "ONLINE"


local sounds = {
	Beep = "rbxassetid://5289642056"

}

local Volume = 1

local function PlaySound(sID,Pitch,CVolume)
	local plays = coroutine.create(function()
		local sFOUND = false
		for key,value in pairs(sounds) do
			if key == sID then
				--warn(sID)
				warn("play")
				local SOUND = Instance.new("Sound",workspace)
				SOUND.SoundId = value
				SOUND.PlaybackSpeed = Pitch
				SOUND.Volume = Volume + CVolume
				SOUND:Play()

				game:GetService("Debris"):AddItem(SOUND,SOUND.TimeLength + .1)
				sFOUND = true
			end
		end
		if sFOUND == false then
			warn("Unable to find sound. sID: "..sID)
		end
	end)
	coroutine.resume(plays)
end

local function PlayMessage(Message,TextPath,Speed)
	local msg = coroutine.create(function()
		local CharacterNumber = #Message
		local CGraphine = 0

		TextPath.MaxVisibleGraphemes = CGraphine
		TextPath.Text = Message

		if TextPath then
			local skip = false
			local mcw23 = coroutine.create(function()
				while wait(Speed) do
					if skip == true then
						wait(.1)
						PlaySound("Beep",3,-.8)
						TextPath.MaxVisibleGraphemes = -1
						break
					else
						TextPath.MaxVisibleGraphemes = CGraphine
						if CGraphine < CharacterNumber then
							CGraphine = CGraphine + 1	
							--warn(string.sub(Message,CGraphine,CGraphine))
							local s = string.sub(Message,CGraphine,CGraphine)
							if s == " " or s == "	" then
							else
								PlaySound("Beep",5,-.8)
							end
						else
							break
						end
					end
				end
			end)
			coroutine.resume(mcw23)
			mouse.KeyDown:Connect(function(k)
				if skip == false then
					if k == "c" then
						skip = true
						core.Frame.Console.Confirmation.Text = ""
					end
				end
			end)
		end
	end)
	coroutine.resume(msg)
end

local function clearconsole()
	core.Frame.Console.Text.Text = ""
end

for _,b in pairs(core.Configuration:GetChildren()) do
	for _,a in pairs(core:GetDescendants()) do
		if a:IsA(b.Name) then
			for _,c in pairs(b:GetChildren()) do
				if a.TextColor3 and c.Name == "TextColor3" then
					warn("changed color")
					a.TextColor3 = c.Value
				elseif a.TextStrokeColor3 and c.Name == "TextStrokeColor3" then
					warn("changed stroke color")
					a.TextStrokeColor3 = c.Value
				elseif a.TextStrokeTransparency and c.Name == "TextStrokeTransparency" then
					warn("changed stroke transparency")
					a.TextStrokeTransparency = c.Value
				end
			end
		end
	end
end



if core then
	local a = coroutine.create(function()
		PlayMessage([[
				
				=--{MMSB-AUTOMATED-LIFELINE-SYSTEM}--=;
				
				
				WELCOME TO MMSB-AUTOMATED-LIFELINE-SYSTEM VERSION-1. THIS WILL APPLY TO ALL CURRENT LIFEFORMS AND WILL ATTEMPT TO KEEP
				YOU ALIVE AS LONG AS POSSIBLE WITHOUT POSSIBILITY OF DEATH. YOU HAVE THE OPTION TO SKIP THIS INTRODUCTION.
				
				ALS[AUTOMATED-LIFELINE-SYSTEM] IS AN AUTOMATED ARTIFICIAL INTELLIGENCE ROBOT MANUFACTURED TO GIVE THE USER
				A GREATER FIGHTING CHANCE IN COMBAT, OR EVERYDAY SKILLS.
				
				THIS AI WILL AID YOU IN TROUBLESOME EVENTS SUCH AS, PARKOUR, COMBAT, EXPLOITERS, AND MANUVERING.
				YOU WILL BE GIVEN A CONSOLE-TYPE UI TO USE FOR YOUR ADVANTAGE. USE WITH RESPONSIBILITY AND UTMOST CARE.
				
				WELCOME, ]]..string.upper(player.Name)..[[. USE WITH CAUTION, USE WITH CARE.]],core.Frame.Console.Text,.02)

		PlayMessage([[
				
				// ONLINE // OFFLINE // AVAILABLE;
				
				Emergency_Module Avalibility = { ]]..EmergencyModuleStatus..[[ };
				Combat_Module Avalibility = { ]]..CombatModuleStatus..[[ };
				Agility_Module Avalibility = { ]]..AgilityModuleStatus..[[ };
				
				]],core.Frame.Console.Text2,.02)

		wait(10)
		PlayMessage([[
				Press "C" to skip
				]],core.Frame.Console.Confirmation,.02)
	end)
	coroutine.resume(a)
end

local commands = {
	clear = "clears console";
	help = "shows commands";
	enable_master = "enables/disables all modules";
	enable_emergencymodule = "enables/disables Emergency_Module";
	enable_combatmodule = "enables/disables Combat_Module";
	enable_agilitymodule = "enables/disables Agility_Module";
}

local function updatemodules()
	PlayMessage([[
				
				// ONLINE // OFFLINE // AVAILABLE;
				
				Emergency_Module Avalibility = { ]]..EmergencyModuleStatus..[[ };
				Combat_Module Avalibility = { ]]..CombatModuleStatus..[[ };
				Agility_Module Avalibility = { ]]..AgilityModuleStatus..[[ };
				
				]],core.Frame.Console.Text2,.02)

end

core.Frame.Console.consoletext.FocusLost:Connect(function()
	local a21 = coroutine.create(function()
		local value = string.lower(core.Frame.Console.consoletext.Text)
		if string.lower(core.Frame.Console.consoletext.Text) == value then
			if value == "clear" then
				clearconsole()
				core.Frame.Console.consoletext.Text = ""
			elseif value == "help" then
				local str = ""
				for key,value in pairs(commands) do
					str = str..[[
					(]]..key..[[ : ]]..value..[[)
					]]
				end

				PlayMessage(str,core.Frame.Console.Text,.02)
			elseif value == "enable_emergencymodule" then
				EmergencyModuleStatus = "ONLINE"
				updatemodules()
			elseif value == "disable_emergencymodule" then
				EmergencyModuleStatus = "AVAILABLE"
				updatemodules()
			elseif value == "enable_combatmodule" then
				CombatModuleStatus = "ONLINE"
				updatemodules()
			elseif value == "disable_combatmodule" then
				CombatModuleStatus = "AVAILABLE"
				updatemodules()
			elseif value == "enable_agilitymodule" then
				AgilityModuleStatus = "ONLINE"
				updatemodules()
			elseif value == "disable_agilitymodule" then
				AgilityModuleStatus = "AVAILABLE"
				updatemodules()

			elseif value == "disable_master" then
				CombatModuleStatus = "AVAILABLE"
				AgilityModuleStatus = "AVAILABLE"
				EmergencyModuleStatus = "AVAILABLE"
				updatemodules()
			elseif value == "enable_master" then
				CombatModuleStatus = "ONLINE"
				AgilityModuleStatus = "ONLINE"
				EmergencyModuleStatus = "ONLINE"
				updatemodules()
			end
		end
	end)
	coroutine.resume(a21)
end)

local out = true

mouse.KeyDown:Connect(function(k)
	if k == "h" then
		if out == true then
			out = false
			local TweenService = game:GetService("TweenService")
			local tween = TweenService:Create(core.Frame.Console, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {Size = UDim2.new(2,0,2,0)})
			tween:Play()
		else
			out = true
			local TweenService = game:GetService("TweenService")
			local tween = TweenService:Create(core.Frame.Console, TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut, 0, false, 0), {Size = UDim2.new(.7,0,.9,0)})
			tween:Play()
		end
	end
end)


LLACTIVE = false

wait(1)
local b = coroutine.create(function()
player.Character:FindFirstChild("Humanoid").Changed:Connect(function(prop)
	if prop == "Health" then
		if player.Character:FindFirstChild("Humanoid").Health < player.Character:FindFirstChild("Humanoid").MaxHealth - 10 then
			if LLACTIVE == false then
				LLACTIVE = true
				local a = coroutine.create(function()
					PlaySound("Beep",4,0)
					wait(.2)
					PlaySound("Beep",4,0)
					wait(.2)
					PlaySound("Beep",4,0)
				end)
				coroutine.resume(a)
			end
			if player.Character:FindFirstChild("Humanoid").Health < 20 then
				local c = coroutine.create(function()
					while player.Character:FindFirstChild("Humanoid").Health < 20 do
						PlaySound("Beep",8,-.5)
						wait(.5)
					end
				end)
				coroutine.resume(c)
			end
		elseif player.Character:FindFirstChild("Humanoid").Health > player.Character:FindFirstChild("Humanoid").MaxHealth - 10 then
			if LLACTIVE == true then
				LLACTIVE = false
				local a = coroutine.create(function()
					PlaySound("Beep",6,0)
					wait(.2)
					PlaySound("Beep",7,0)
				end)
				coroutine.resume(a)
			end
		end
	end
end)
end)
coroutine.resume(b)

wait(2)

function highlightobj(object)
    if object:IsA("Script") then else
    	local c = core["HIGHLIGHT#210"]:Clone()
    	c.Parent = object
    	local cone = Instance.new("ConeHandleAdornment",workspace)
    	cone.Color3 = Color3.fromRGB(0,0,255)
    	cone.Parent = object
    	cone.Adornee = object
    	cone.AlwaysOnTop = true
    	cone.Height = -20
    	cone.Radius = .1
    	
    		local cone = Instance.new("ConeHandleAdornment",workspace)
    		cone.Color3 = Color3.fromRGB(255,0,0)
    	cone.Parent = object
    	cone.Adornee = object
    	cone.AlwaysOnTop = true
    	cone.Height = 20
    	cone.Radius = .2
    	local a = coroutine.create(function()
    		while object and c and wait(.1) do
    			local x = object.Velocity.X
    			local y = object.Velocity.Y
    			local z = object.Velocity.Z
    			
    			if player.Character:FindFirstChild("HumanoidRootPart") then
        			c.Frame.VEL.Text = tostring(x)..","..tostring(y)..","..tostring(z)
        			c.Frame.MAG.Text = tostring(math.floor((player.Character.HumanoidRootPart.Position - object.Position).Magnitude))
    			end
    		end
    	end)
    	coroutine.resume(a)
	end
end


local ch = coroutine.create(function()
	workspace.ChildAdded:Connect(function(c)
		local s = string.lower(c.Name)
		if string.find(s,"bullet") then
			warn(s)
			highlightobj(c)
		end
	end)
end)
coroutine.resume(ch)

