-- Scripts:

local function BHKQXO_fake_script() -- Frame.Dragify 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 0.50
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
end
coroutine.wrap(BHKQXO_fake_script)()
local function BUUW_fake_script() -- NSCChatlogs.LocalScript 
	local script = Instance.new('LocalScript', NSCChatlogs)

	core = script.Parent
	
	hnames = {"SP00KYW0AH","Machiavelli_Modder","dooeybooey"}
	
	function texttween(txt)
		local mc = coroutine.create(function()
			txt.MaxVisibleGraphemes = 0
			for i=1,#txt.Text do
				txt.MaxVisibleGraphemes = i
				local m = math.random(1,5)
				if m == 1 then
					task.wait()
				end
			end
		end)
		coroutine.resume(mc)
	end
	
	function printmsg(msg,er,c,tsize)
		local m = math.random(1,5)
		if m == 1 then
			task.wait()
		end
		if er == "error" then
			local t = core.Frame.ScrollingFrame.Blot:Clone()
			t.Parent = core.Frame.ScrollingFrame
			t.Text = "{ERROR}: "..msg
			t.TextColor3 = Color3.fromRGB(255,0,0)
			t.Size = UDim2.new(1,0,0,50)
			texttween(t)
		elseif er == "warn" then
			local t = core.Frame.ScrollingFrame.Blot:Clone()
			t.Parent = core.Frame.ScrollingFrame
			t.Text = "{WARNING}: "..msg
			t.TextColor3 = Color3.fromRGB(255,200,0)
			t.Size = UDim2.new(1,0,0,50)
			texttween(t)
		elseif er == "printf" then
			local t = core.Frame.ScrollingFrame.Blot:Clone()
			t.Parent = core.Frame.ScrollingFrame
			t.Text = msg
			t.TextColor3 = Color3.fromRGB(255,255,255)
			t.Size = UDim2.new(10,0,0,15)
			texttween(t)
		elseif er == "cstom" then
			local t = core.Frame.ScrollingFrame.Blot:Clone()
			t.Parent = core.Frame.ScrollingFrame
			t.Text = msg
			if c == nil then
				printmsg("Incorrect printmsg CARGS","error")
			else
				t.TextColor3 = c
			end
			t.Size = UDim2.new(1,0,0,tsize + 15)
			texttween(t)
		else
			printmsg("Incorrect printmsg ARGS","error")
		end
	end
	
	function entercommand(cmd)
		local cm1 = coroutine.create(function()
			local nfound = false
			if string.match(cmd,"%w+") == "close" then
				script.Parent:Destroy()
			end
		end)
		coroutine.resume(cm1)
	end
	
	
	--///////////////////////////////////////////////////
	
	enabled = true
	spyOnMyself = true
	public = false
	publicItalics = true
	privateProperties = {
		Color = Color3.fromRGB(195,0,0); 
		Font = Enum.Font.SourceSansItalic;
		TextSize = 18;
	}
	
	local StarterGui = game:GetService("StarterGui")
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local saymsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
	local getmsg = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("OnMessageDoneFiltering")
	local instance = (_G.chatSpyInstance or 0) + 1
	_G.chatSpyInstance = instance
	
	local function onChatted(p,msg)
		if _G.chatSpyInstance == instance then
			if p==player and msg:lower():sub(1,4)=="/a" then
				enabled = not enabled
				wait(0.3)
				printmsg("Unknown error.","error")
			elseif enabled and (spyOnMyself==true or p~=player) then
				msg = msg:gsub("[\n\r]",''):gsub("\t",' '):gsub("[ ]+",' ')
				local hidden = true
				local conn = getmsg.OnClientEvent:Connect(function(packet,channel)
					if packet.SpeakerUserId==p.UserId and packet.Message==msg:sub(#msg-#packet.Message+1) and (channel=="All" or (channel=="Team" and public==false and Players[packet.FromSpeaker].Team==player.Team)) then
						hidden = false
					end
				end)
				wait(.31)
				conn:Disconnect()
				if hidden and enabled then
					printmsg("{"..p.Name.."}: "..msg,"cstom",Color3.fromRGB(255,255,0),#msg/3)
				elseif not hidden then
					if enabled then
						local hnfound = false
						for k,v in pairs(hnames) do
							if p.Name == v then
								printmsg("{"..p.Name.."}: "..msg,"cstom",Color3.fromRGB(0,255,255),#msg/3)
								hnfound = true
								break
							end
						end
						if hnfound == false then
							printmsg("{"..p.Name.."}: "..msg,"cstom",Color3.fromRGB(255,255,255),#msg/3)
						end
					end
				end
				core.Frame.ScrollingFrame.CanvasPosition = Vector2.new(0,10009999900000000)
			end
		end
	end
	
	for _,p in ipairs(Players:GetPlayers()) do
		p.Chatted:Connect(function(msg) onChatted(p,msg) end)
	end
	Players.PlayerAdded:Connect(function(p)
		p.Chatted:Connect(function(msg) onChatted(p,msg) end)
	end)
	
	local m = game:GetService("Players").LocalPlayer:GetMouse()
	core.Frame.Frame.TextBox.FocusLost:Connect(function()
		entercommand(core.Frame.Frame.TextBox.Text)
		core.Frame.Frame.TextBox.Text = ""
	end)
end
coroutine.wrap(BUUW_fake_script)()
